{
  "name": "handelsregister",
  "version": "1.0.0",
  "description": "Node.js SDK for accessing German company registry (Handelsregister) data via handelsregister.ai API",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "bin": {
    "handelsregister": "./cli/index.js"
  },
  "scripts": {
    "build": "npm run clean && npm run build:ts && npm run build:cli",
    "build:ts": "tsc && tsc --module esnext --outDir dist/esm",
    "build:cli": "tsc cli/index.ts --outDir . --module commonjs",
    "clean": "rimraf dist cli/*.js cli/*.d.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts cli/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\" \"cli/**/*.ts\" \"tests/**/*.ts\"",
    "prepublishOnly": "npm run build && npm test",
    "prepare": "npm run build"
  },
  "keywords": [
    "handelsregister",
    "german-companies",
    "company-registry",
    "api-client",
    "business-data",
    "germany",
    "sdk",
    "typescript"
  ],
  "author": "handelsregister.ai",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "commander": "^11.0.0",
    "dotenv": "^16.3.0",
    "csv-parse": "^5.5.0",
    "csv-stringify": "^6.4.0",
    "xlsx": "^0.18.5",
    "cli-progress": "^3.12.0",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.3"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jest": "^29.5.0",
    "@types/cli-progress": "^3.11.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.2.0",
    "rimraf": "^5.0.0",
    "prettier": "^3.0.0",
    "nock": "^13.3.0"
  },
  "files": [
    "dist",
    "cli/*.js",
    "cli/*.d.ts",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/handelsregister/handelsregister-js.git"
  },
  "bugs": {
    "url": "https://github.com/handelsregister/handelsregister-js/issues"
  },
  "homepage": "https://handelsregister.ai"
}